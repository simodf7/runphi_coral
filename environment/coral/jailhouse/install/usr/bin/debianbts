#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'python-debianbts==2.8.2','console_scripts','debianbts'
__requires__ = 'python-debianbts==2.8.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-debianbts==2.8.2', 'console_scripts', 'debianbts')()
    )
